{
  "name": "uniswap-v3-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/graphprotocol/uniswap-v3-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "foo": "mustache src/configs/$NETWORK.json src/networkConstants/constants.template.ts",
    "prepare-const": "mustache src/configs/$NETWORK.json src/networkConstants/constants.template.ts > src/networkConstants/constants.ts",
    "prepare-yaml": "mustache src/configs/$NETWORK.json subgraph.template.yaml  subgraph.yaml",
    "deploy": "graph deploy $SUBGRAPH_NAME --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "sf-deploy": "graph deploy --product hosted-service streamingfast/uni-v3-minimal"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.0",
    "@graphprotocol/graph-ts": "^0.20.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "mustache": "^4.2.0"
  }
}
